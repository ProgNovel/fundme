{
  "name": "fundme",
  "version": "0.0.5",
  "description": "A simple but powerful client-side library to manage monetization on the web.",
  "main": "./dist/fundme-cjs.js",
  "module": "./dist/fundme.mjs",
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.3",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "version": "npm run build",
    "build": "del dist /Q && rollup -c && npm run compress-iife && npm run create-compat && npm run create-cjs-compat && npm run compress-amd",
    "pre-commit": "npm run build && git add .",
    "compress-amd": "uglifyjs --compress --mangle -o dist/fundme-amd.min.js dist/fundme-amd.js",
    "compress-iife": "uglifyjs --compress --mangle -o dist/fundme-iife.min.js dist/fundme-iife.js",
    "create-cjs-compat": "babel dist/fundme-cjs.js | uglifyjs --compress --mangle > dist/fundme-cjs-compat.min.js",
    "create-compat": "babel dist/fundme-iife.js | uglifyjs --compress --mangle > dist/fundme-iife-compat.min.js",
    "test": "jest --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ProgNovel/fundme.git"
  },
  "keywords": [
    "Web Monetization API",
    "Donation",
    "Ads",
    "Revenue",
    "Coil",
    "Split revenue"
  ],
  "author": "ProgNovel",
  "license": "Apache-2.0"
}
